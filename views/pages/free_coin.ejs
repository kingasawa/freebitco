<div class="ui inverted fixed menu">
  <div class="header item">FREEBITCO</div>
  <div class="right menu">
    <a class="item">FREE BTC</a>
    <a class="item" style="color:#21ba45">MULTIPLY BTC</a>
    <a class="item">BETTING</a>
    <a class="item" style="color:#fbbd08">GOLDEN TICKET</a>
    <a class="item">EARN BTC</a>
    <a class="item">LOTTERY</a>
    <a class="item">CONTEST</a>
    <a class="item">REFER</a>
    <a class="item">MORE</a>
